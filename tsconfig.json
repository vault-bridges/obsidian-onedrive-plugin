{
	"compilerOptions": {
		"allowJs": true,
		"baseUrl": ".",
		"esModuleInterop": true,
		"importHelpers": true,
		"inlineSourceMap": true,
		"inlineSources": true,
		"isolatedModules": true,
		"lib": ["DOM", "ES5", "ES6", "ES7", "ESNext"],
		"module": "ESNext",
		"moduleResolution": "node",
		"noEmit": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"target": "ES6"
	},
	"include": ["**/*.ts"]
}
